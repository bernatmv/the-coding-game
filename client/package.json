{
  "name": "webpack-2-example-4",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack",
    "start": "webpack-dev-server -d --hot --inline --colors --port 9009 --bail --compress",
    "test": "jest --watchAll --no-cache"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "<rootDir>/preprocessor.js"
    },
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testRegex": "<rootDir>(__tests__/).*\\.(test|spec)\\.(ts|js|tsx|jsx)$"
  },
  "devDependencies": {
    "@types/classnames": "0.0.32",
    "@types/jest": "18.1.1",
    "@types/node": "7.0.10",
    "@types/react": "15.0.18",
    "@types/react-dom": "0.14.23",
    "@types/react-redux": "4.4.38",
    "@types/react-router": "4.0.1",
    "@types/react-router-redux": "4.0.43",
    "@types/redux-actions": "1.2.3",
    "@types/rx": "4.1.1",
    "@types/rx-dom": "7.0.0",
    "@types/webpack": "2.2.12",
    "@types/webpack-env": "1.13.0",
    "awesome-typescript-loader": "^3.1.3",
    "classnames": "2.2.5",
    "css-loader": "0.28.4",
    "extract-text-webpack-plugin": "2.1.2",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "20.0.4",
    "less": "2.7.2",
    "less-loader": "4.0.4",
    "react-addons-test-utils": "15.5.1",
    "react-hot-loader": "1.3.1",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.18.2",
    "tslint": "5.4.3",
    "typescript": "2.3.4",
    "url-loader": "0.5.8",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5",
    "webpack-hot-middleware": "2.17.1"
  },
  "dependencies": {
    "react": "15.5.2",
    "react-dom": "15.5.2",
    "react-redux": "5.0.3",
    "react-router": "4.0.0",
    "redux": "3.6.0",
    "redux-actions": "2.0.1",
    "rx": "4.1.0",
    "rx-dom": "7.0.3"
  }
}
